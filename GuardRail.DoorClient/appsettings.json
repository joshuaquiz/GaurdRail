{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "BuzzerConfiguration": {
        "Pin": 6
    },
    "KeypadConfiguration": {
        "Pins": [ 17, 27, 22, 5, 23, 24, 25, 16 ],
        "KeypadTimeout": "00:00:03",
        "SubmitKey": "#"
    },
    "x": "y", 
    "LightConfiguration": {
        "RedPin": 21,
        "GreenPin": 26
    },
    "UdpConfiguration": {
        "Port": 8989
    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            { "Name": "Console" }
        ],
        "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
        "Properties": {
            "Application": "GuardRail.DoorClient"
        }
    }
}